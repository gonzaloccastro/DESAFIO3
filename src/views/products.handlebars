<div>
    <h1>¡Hola, estos son nuestros productos! </h1>

<div>
    <div><h1>Estás conectado como: {{this.first_name}}</h1></div>
</div>

</div>
<table>
  <tr>
    <th>ID</th>
    <th>Título</th>
    <th>Descripción</th>
    <th>Código</th>
    <th>Precio</th>
    <th>Stock</th>
    <th>Categoría
    </th>
  </tr>
{{#each products}}
  <tr>
    <td>{{this.id}}</td>
    <td>{{this.title}}</td>
    <td>{{this.description}}</td>
    <td>{{this.code}}</td>
    <td>{{this.price}}</td>
    <td>{{this.stock}}</td>
    <td>{{this.category}}</td>
  </tr>
{{/each}}

</table>