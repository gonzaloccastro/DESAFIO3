<div>
    <div><h1>Hola, registrate para entrar:</h1></div>
  <form id="myForm" method="GET" class="speaker-form">
    <div class="form-row">
      <label for="full-name">Nombre</label>
      <input id="full-name" name="first_name" type="text" required>
    </div>
    <br>
    <div class="form-row">
        <label for="full-user">Apellido</label>
        <input id="lastName" name="last_name" type="text" required>
    </div>
    <br>
    <div class="form-row">
      <label for="email">Correo electronico</label>
      <input
        id="email"
        name="email"
        type="email"
        required
      >
      </div>
      <br>
      <div class="form-row">
        <label for="full-user">Age</label>
        <input id="age" name="age" type="text" required>
      </div>
      <br>
      <div class="form-row">
        <label for="full-user">Password</label>
        <input id="password" name="password" type="password" required>
      </div>
    </div>

    <div class="form-row">
      <br>
      <button id="signup" type="submit" class="signup">
        Crear usuario
      </button>
    </div>
  </form>
    <br>
    <a href="http://localhost:8080/"> <button id="register">Ya tengo un usuario</button> </a>

</div>
